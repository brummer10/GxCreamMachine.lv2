@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_CreamMachine_#_CreamMachine_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxcreammachine.html> ;
        modgui:stylesheet <modgui/stylesheet-gxcreammachine.css> ;
        modgui:screenshot <modgui/screenshot-gxcreammachine.png> ;
        modgui:thumbnail <modgui/thumbnail-gxcreammachine.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "GxCreamMachine" ;
        modgui:model "combo-model-001" ;
        modgui:panel "0550" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "LEVEL" ;
            lv2:name "LEVEL" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "BASS" ;
            lv2:name "BASS" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "TREBLE" ;
            lv2:name "TREBLE" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "VOLUME" ;
            lv2:name "VOLUME" ;
        ] ;
    ] .